{
  "name": "vexel-missed-call-bot",
  "version": "1.0.0",
  "description": "Automated missed call recovery bot for dental practices",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node ./db/migrate.js",
    "migrate:up": "npm run migrate",
    "backup": "bash ../scripts/backup-db.sh",
    "lint": "echo 'No lint configured'",
    "test:unit": "echo 'No unit tests yet'",
    "test:integration": "echo 'No integration tests yet'",
    "test:e2e": "bash ./test-smoke.sh || true",
    "secret-scan": "node ./scripts/secret-scan.js"
  },
  "keywords": [
    "twilio",
    "missed-call",
    "sms",
    "automation",
    "dental"
  ],
  "author": "Vexel Logic",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "twilio": "^4.20.0",
    "@supabase/supabase-js": "^2.39.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.0",
    "express-rate-limit": "^6.8.0",
    "cookie-parser": "^1.4.6",
    "@sendgrid/mail": "^7.8.0",
    "stripe": "^12.15.0",
    "pino": "^8.12.0",
    "@sentry/node": "^7.27.0",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "node-pg-migrate": "^6.3.4",
    "k6": "^0.47.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
