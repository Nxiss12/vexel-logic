      "position": [450, 300],
      "credentials": {
        "twilioApi": {
          "id": "3",
          "name": "Twilio account"
        }
      }
    },
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "sms-reply",
        "responseMode": "responseNode",
        "options": {}
      },
      "id": "customer-reply-trigger",
      "name": "Customer Replies (SMS)",
