      "typeVersion": 1,
      "position": [250, 500],
      "webhookId": "plumber-sms-reply"
    },
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "={{$json.Body.toLowerCase()}}",
              "operation": "contains",
              "value2": "emergency"
            },
            {
              "value1": "={{$json.Body.toLowerCase()}}",
              "operation": "contains",
              "value2": "urgent"
            },
            {
              "value1": "={{$json.Body.toLowerCase()}}",
              "operation": "contains",
              "value2": "leak"
            }
          ]
        },
        "combineOperation": "any"
      },
      "id": "is-urgent",
      "name": "Is Urgent?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [450, 500]
    },
    {
      "parameters": {
        "fromNumber": "={{$env.TWILIO_PHONE_NUMBER}}",
        "toNumber": "={{$json.From}}",
        "message": "üö® Got it - this sounds urgent! We'll call you back within 30 minutes. If it's a burst pipe, turn off your main water valve now.",
        "options": {}
      },
      "id": "urgent-response",
