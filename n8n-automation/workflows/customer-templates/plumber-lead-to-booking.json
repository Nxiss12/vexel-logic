      "type": "n8n-nodes-base.twilio",
      "typeVersion": 1,
      "position": [650, 400],
      "credentials": {
        "twilioApi": {
          "id": "3",
          "name": "Twilio account"
        }
      }
    },
    {
      "parameters": {
        "url": "={{$env.SLACK_WEBHOOK_URL}}",
